use lexer

struct File {
    path: text,
    done: boolean,
}

struct Definition {
    name: text,
    file: u16,
    line: u32,
}

struct Type {
    name: text,
}

struct Parser {
    file: vector<File>,
    definitions: vector<Definition>,
    types: vector<Type>,
}
