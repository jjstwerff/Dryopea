fn main() {
    x = [1, 3, 6, 9];
    b = "";
    for v in x {
        if !v#first {
            b += " ";
        }
        b += "{v#index}:{v}"
    }
    assert(b == "0:1 1:3 2:6 3:9", "result {b}");
    x += [12, 14, 15];
    for v in x if v % 3 != 0 {
        v#remove;
    }
    assert("{x}" == "[3,6,9,12,15]", "result {x}");
}
// sub vectors via range index
